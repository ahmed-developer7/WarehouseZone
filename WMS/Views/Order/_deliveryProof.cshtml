@model List<Ganedata.Core.Entities.Domain.OrderProofOfDelivery>
<div id="invoice-preview-container">
    <div class="col-md-12 p-0 invoice-preview-pane">

        <div class="col-md-12 p-0">
            <div class="invoice-header-accountinfo col-md-7">
                <table class="table">
                 @if (Model.Count > 0)
                 {
                     foreach (var item in Model)
                     {


                    <tr>
                        <td><label><b>Signature Name:</b></label>@item.SignatoryName</td>
                        <td><label><b>Date Created:</b></label>@item.DateCreated.ToShortDateString()</td>
                        <td><label><b>Cases Delivered:</b></label>@item.NoOfCases</td>
                        <td><label><b>Signature:</b></label><img src="~/UploadedFiles/SignatureProofs/@item.FileName" style="width:200px; height:100px" /></td>

                    </tr>
                     }
                 }

                </table>
            </div>
        </div>
    </div>
</div>
