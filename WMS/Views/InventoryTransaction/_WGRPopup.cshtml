@Html.DevExpress().PopupControl(
    settings =>
    {
        settings.Name = "ModelGoodsReturn";
        settings.LoadContentViaCallback = LoadContentViaCallback.OnFirstShow;
        settings.CallbackRouteValues = new { Controller = "InventoryTransaction", Action = "_GoodsReturn", Method = "GET" };
        settings.Width = 700;
        settings.Height = 500;
        settings.AllowDragging = true;
        settings.CloseAction = CloseAction.CloseButton;
        settings.CloseOnEscape = true;
        settings.PopupAnimationType = AnimationType.None;
        settings.ShowHeader = false;
        settings.Modal = true;
        settings.AutoUpdatePosition = true;
        settings.PopupHorizontalAlign = PopupHorizontalAlign.WindowCenter;
        settings.PopupVerticalAlign = PopupVerticalAlign.WindowCenter;
        settings.ScrollBars = System.Web.UI.WebControls.ScrollBars.Vertical;
        settings.EnableClientSideAPI = true;
        settings.ClientSideEvents.BeginCallback = "beginGRCallBack";
        settings.ClientSideEvents.EndCallback = "function(s,e){ initialiseSeriesGenerateActions(s,e);}";

        //settings.ClientSideEvents.CloseUp = "function(s, e){ ASPxClientEdit.ClearEditorsInContainer(null, '', true); }";
    }).GetHtml()