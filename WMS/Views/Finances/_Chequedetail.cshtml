@model List<Ganedata.Core.Entities.Domain.AccountTransactionFile>

@if (Model.Count > 0)
{
    foreach (var item in Model)
    {
        string imreBase64Data = Convert.ToBase64String(item.FileContent);
        string imgDataURL = string.Format("data:image/png;base64,{0}", imreBase64Data);
        <div class="row">
            <img src="@imgDataURL" class="img-thumbnail" style="width:400px; height:300px;"/>

        </div>

    }
}


